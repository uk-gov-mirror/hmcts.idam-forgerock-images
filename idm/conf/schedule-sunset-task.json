{
    "enabled" : true,
    "persisted" : true,
    "recoverable" : false,
    "misfirePolicy" : "fireAndProceed",
    "schedule" : "0 0 * * * ?",
    "repeatInterval" : 0,
    "repeatCount" : 0,
    "type" : "cron",
    "invokeService" : "taskscanner",
    "invokeContext" : {
        "waitForCompletion" : false,
        "numberOfThreads" : 5,
        "scan" : {
            "_queryFilter" : "((/sunset/date pr) AND !(/sunset/date eq \"\") AND (/sunset/date lt \"${Time.now}\") AND !(/sunset/task-completed pr))",
            "object" : "managed/user",
            "taskState" : {
                "started" : "/sunset/task-started",
                "completed" : "/sunset/task-completed"
            },
            "recovery" : {
                "timeout" : "openidm-admin"
            }
        },
        "task" : {
            "script" : {
                "type" : "text/javascript",
                "file" : "script/sunset.js"
            }
        }
    },
    "invokeLogLevel" : "info",
    "startTime" : null,
    "endTime" : null,
    "concurrentExecution" : false,
    "managedObject" : "user",
    "scriptProperty" : "sunset",
    "timeZone" : "Europe/London"
}